#ifndef DigitalOutPin_h
#define DigitalOutPin_h

#include "Arduino.h"

class DigitalOutPin
{
  public:
    DigitalOutPin(int pin);
    void on();
    void of();
  private:
    int _pin;
    boolean _isOn;
    boolean _isDebug;;
    boolean _isMock;
};

#endif

